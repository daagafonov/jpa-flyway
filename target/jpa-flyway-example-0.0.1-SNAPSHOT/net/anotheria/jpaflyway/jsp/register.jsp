<form action="doRegister" method="post" autocomplete="off">
	<table>
		<tr>
			<th>Login :</th>
			<td><input type="text" name="login"/></td>
		</tr>
		<tr>
			<th>Password :</th>
			<td><input type="password" name="password"/></td>
		</tr>
		<tr>
			<th>Gender :</th>
			<td><input type="text" name="gender"/></td>
		</tr>
		<tr>
			<th>Email :</th>
			<td><input type="text" name="email"/></td>
		</tr>
		<tr>
			<td colspan="2">
				<input type="submit" />
			</td>
		</tr>
	</table>
</form>